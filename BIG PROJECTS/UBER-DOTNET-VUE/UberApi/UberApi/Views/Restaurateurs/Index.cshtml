@model IEnumerable<UberApi.Models.EntityFramework.Restaurateur>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrenomUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telephone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MotDePasseUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrenomUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telephone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MotDePasseUser)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdRestaurateur">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdRestaurateur">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdRestaurateur">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
